{
  "name": "@canivete-suico/root",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap --use-workspaces",
    "dev": "start-storybook -p 5555",
    "build": "lerna exec --parallel -- babel --extensions .js,.jsx  --root-mode upward src -d lib --ignore **/*.stories.js,**/*.spec.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@storybook/addon-docs": "^6.3.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "lerna": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "webpack": "^5.58.2"
  }
}
